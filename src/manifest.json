{
  "name": "__ww_monitor__",
  "version": "1.0",
  "description": "fx monitor",
  "icons": {
    "16": "image/FX16.png",
    "48": "image/FX48.png",
    "128": "image/FX128.png"
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "webRequest",
    "<all_urls>",
    "background",
    "storage",
    "notifications"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://mail.wolwobiotech.com/*"
      ],
      "js": [
        "javascript/jquery-2.1.1.js",
        "javascript/Utils.js",
        "javascript/WwAnalyzer.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "background" : {
    "page": "views/background.html"
  },
  "web_accessible_resources": [
    "image/*"
  ],
  "manifest_version": 2
}
